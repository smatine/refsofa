tags: [], isLoading: true};\\r\\n    this.remove = this.remove.bind(this);\\r\\n  }\\r\\n\\r\\n  componentDidMount() {\\r\\n    this.setState({isLoading: true});\\r\\n\\r\\n    fetch(`/subnets/${this.props.match.params.id}/tags`)\\r\\n      .then(response => response.json())\\r\\n      .then(data =>   this.setState({tags: data, isLoading: false}));\\r\\n  }\\r\\n\\r\\n  async remove(accId, id) {\\r\\n    await fetch(`/subnets/${accId}/tags/${id}`, {\\r\\n      method: 'DELETE',\\r\\n      headers: {\\r\\n        'Accept': 'application/json',\\r\\n        'Content-Type': 'application/json'\\r\\n      }\\r\\n    }).then(() => {\\r\\n      let updateTagSubnet = [...this.state.tags].filter(i => i.id !== id);\\r\\n      this.setState({tags: updateTagSubnet});\\r\\n    });\\r\\n  }\\r\\n\\r\\n  render() {\\r\\n    const {tags, isLoading} = this.state;\\r\\n\\r\\n    if (isLoading) {\\r\\n      return <p>Loading...</p>;\\r\\n    }\\r\\n\\r\\n    const tagList = tags.map(tag => {\\r\\n      \\r\\n      const link = \\\"/subnets/\\\" + tag.subnet.id; \\r\\n\\r\\n      return <tr key={tag.id}>\\r\\n        <td style={{whiteSpace: 'nowrap'}}>{tag.id}</td>\\r\\n\\r\\n\\r\\n        <td>{tag.key}</td>\\r\\n        <td>{tag.value}</td>\\r\\n\\r\\n        <td>{tag.text}</td>\\r\\n        <td><a href={link}>{tag.subnet.id}</a></td>\\r\\n        <td>\\r\\n          <ButtonGroup>\\r\\n            <Button size=\\\"sm\\\" color=\\\"primary\\\" tag={Link} to={\\\"/subnet/\\\" + tag.subnet.id  + \\\"/tags/\\\" + tag.id}>Edit</Button>\\r\\n            <Button size=\\\"sm\\\" color=\\\"danger\\\" onClick={() => this.remove(tag.subnet.id, tag.id)}>Delete</Button>\\r\\n          </ButtonGroup>\\r\\n        </td>\\r\\n      </tr>\\r\\n    });\\r\\n\\r\\n    const add = `/subnet/${this.props.match.params.id}/tags/new`;\\r\\n    const subnet = `/subnets`;\\r\\n\\r\\n    return (\\r\\n      <div>\\r\\n        <AppNavbar/>\\r\\n        <Container fluid>\\r\\n          <div className=\\\"float-righ