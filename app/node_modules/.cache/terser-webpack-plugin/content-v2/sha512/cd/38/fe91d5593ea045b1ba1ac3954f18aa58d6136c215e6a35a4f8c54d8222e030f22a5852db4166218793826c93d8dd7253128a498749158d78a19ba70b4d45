anage/' + vpc.cidr.id;\\r\\n\\r\\n      let nacls = '';\\r\\n      if(vpc.nacls)vpc.nacls.map(s => {  \\r\\n        nacls = nacls + s + \\\"| \\\"; \\r\\n      });\\r\\n\\r\\n      let routeTables = '';\\r\\n      if(vpc.routeTables)vpc.routeTables.map(s => {  \\r\\n        routeTables = routeTables + s + \\\"| \\\"; \\r\\n      });\\r\\n\\r\\n      let dhcps = '';\\r\\n      if(vpc.dhcps)vpc.dhcps.map(s => {  \\r\\n        dhcps = dhcps + s + \\\"| \\\"; \\r\\n      });\\r\\n\\r\\n      return <tr key={vpc.id}>\\r\\n        <td style={{whiteSpace: 'nowrap'}}>{vpc.id}</td>\\r\\n\\r\\n\\t\\t    <td>{vpc.name}</td>\\r\\n        <td><a href={cid}>{cidr}</a></td>\\r\\n        <td><a href={link}>{vpc.account.numAccount}: {vpc.account.env}</a></td>\\r\\n\\t\\t    <td>{vpc.text}</td>\\r\\n        <td>{nacls}</td>\\r\\n        <td>{routeTables}</td>\\r\\n\\t\\t    <td>{dhcps}</td>\\r\\n       \\r\\n\\r\\n        <td>\\r\\n          <ButtonGroup>\\r\\n            <Button size=\\\"sm\\\" color=\\\"secondary\\\" tag={Link} to={API_BASE_URL + \\\"/vpc/\\\" + vpc.id + \\\"/tags\\\" }>Tags</Button>&nbsp;&nbsp;\\r\\n            <Button size=\\\"sm\\\" color=\\\"primary\\\" tag={Link} to={API_BASE_URL + \\\"/vpcs/\\\" + vpc.id}>Edit</Button>\\r\\n            <Button size=\\\"sm\\\" color=\\\"danger\\\" onClick={() => this.remove(vpc.account.id, vpc.id)}>Delete</Button>\\r\\n          </ButtonGroup>\\r\\n        </td>\\r\\n      </tr>\\r\\n    });\\r\\n\\r\\n   const add = `${API_BASE_URL}/vpcs/new`;\\r\\n\\r\\n    return (\\r\\n      <div>\\r\\n        <AppNavbar/>\\r\\n        <Container fluid>\\r\\n          <div className=\\\"float-right\\\">\\r\\n            <Button color=\\\"success\\\" tag={Link} to={add}>Add Vpc</Button>\\r\\n          </div>\\r\\n          <h3>Vpc</h3>\\r\\n          <Table className=\\\"mt-4\\\">\\r\\n            <thead>\\r\\n            <tr>\\r\\n              <th width=\\\"20%\\\">Id</th> \\r\\n              <th width=\\\"20%\\\">Name</th>\\r\\n         